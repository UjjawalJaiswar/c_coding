#include<stdio.h>

int main()
{
    unsigned char a,b;
    scanf("%d, %d",a, b);
    printf("%d&%d=%d\n",a, b, a&b);
    printf("%d|%d=%d\n",a, b, a|b);
    printf("%d^%d=%d\n",a, b, a^b);
    printf("~%d=%d\n",a,~a);
    printf("%d<<1=%d\n",b, b<<1);
    printf("%d>>1=%d\n",b, b>>1);
    return 0;
}
